{
//  "compilerOptions": {
//    "target": "ES6",
//    "jsx": "react",
//    "noEmit": true
//  },


  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
//    "noEmit": true,
    "jsx": "react",
    "noImplicitAny": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/ckeditor5-custom-build",
    "node_modules",
    "dev_server.js"
  ]
}
